import math
import os
import random
import re
import sys

def permutationEquation(p):
    result = []
    
    pos = {value: idx+1 for idx, value in enumerate(p)}
    
    for x in range(1, len(p)+1):
        y = pos[pos[x]]
        result.append(y)
    return result


if __name__ == '__main__':
    n = int(input().strip())
    p = list(map(int, input().rstrip().split()))
    result = permutationEquation(p)
    for r in result:
        print(r)
