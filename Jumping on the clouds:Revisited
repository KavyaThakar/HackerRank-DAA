import math
import os
import random
import re
import sys


def jumpingOnClouds(c, k):
    n = len(c)
    energy = 100
    pos = 0
    while True:
        pos = (pos + k) % n
        energy -= 1 + 2 * c[pos]
        if pos == 0:
            break
    return energy


if __name__ == '__main__':
    n, k = map(int, input().split())
    c = list(map(int, input().split()))
    result = jumpingOnClouds(c, k)
    print(result)
