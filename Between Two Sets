import math
import os
import random
import re
import sys

def lcm(x, y):
    return x * y // math.gcd(x, y)

def getTotalX(a, b):
    # Compute LCM of all elements in a
    lcm_a = a[0]
    for i in a[1:]:
        lcm_a = lcm(lcm_a, i)

    
    gcd_b = b[0]
    for i in b[1:]:
        gcd_b = math.gcd(gcd_b, i)

    # Count the numbers between LCM and GCD that divide GCD evenly
    count = 0
    multiple = lcm_a
    while multiple <= gcd_b:
        if gcd_b % multiple == 0:
            count += 1
        multiple += lcm_a

    return count

if __name__ == '__main__':
    fptr = open(os.environ['OUTPUT_PATH'], 'w')

    n, m = map(int, input().rstrip().split())

    arr = list(map(int, input().rstrip().split()))

    brr = list(map(int, input().rstrip().split()))

    total = getTotalX(arr, brr)

    fptr.write(str(total) + '\n')

    fptr.close()
