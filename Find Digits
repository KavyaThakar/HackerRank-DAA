import math
import os
import random
import re
import sys

def findDigits(n):
    count = 0
    for digit in str(n):
        d = int(digit)
        if d != 0 and n % d == 0:
            count += 1
    return count


if __name__ == '__main__':
    t = int(input().strip())
    for _ in range(t):
        n = int(input().strip())
        result = findDigits(n)
        print(result)
